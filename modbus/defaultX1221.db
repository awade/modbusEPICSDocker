## READ IN CHANNELS FROM ACROMAG CARDS TO BROADCAST ##
# Set XXXX to the acromag card identifier
# and YYYY to the experiment identifier
# Change C2 to the lab identifier


####
#Chan_A
record(ai, "C2:ACROMAG_INPUT_XXXX0") #Grab channel off accromag card
{
  field(SCAN, ".1 second")
  field(FLNK, "C2:ACROMAG_INPUT_XXXX0_C.PROC")
  field(DTYP, "asynInt32")
  field(INP, "@asynMask(ADC_Reg_XXXX 0 -16)MODBUS_DATA")
}
record(calc, "C2:YYYY-CHAN_A") #Calibrate voltage and generate soft channel
{
  field(CALC,"A*4.9938E-4+6.2331E-4")
  field(SCAN, ".1 second")
  field(INPA,"C2:ACROMAG_INPUT_XXXX0")
}

